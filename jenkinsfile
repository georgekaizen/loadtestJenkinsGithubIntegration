pipeline {
    agent any
    stages {
        stage('Selfcare Portal Performance Testing') {
            steps {
                echo 'Installing k6'
                sh 'sudo chmod +x setup_k6.sh'
                sh 'sudo ./setup_k6.sh'
                echo 'Running K6 Selfcare Portal Performance Testing...'
                sh 'k6 run samplebcptest.js'
            }
        }
    }
}
